resource "azurerm_public_ip" "jumpbox-pip" {
  allocation_method   = "Dynamic"
  domain_name_label   = "djs-k8s-jumpbox"
  location            = module.k8s-rg.location
  name                = "PIP-JUMPBOX"
  resource_group_name = module.k8s-rg.name
  sku                 = "Basic"
  tags = var.tags
}